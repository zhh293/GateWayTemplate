String bodyEncryptMsg = EncryptUtil.encryptAES(bodyMsg, key);

vars.put(&quot;queryName&quot;, EncryptUtil.encryptAES(queryName, key));
vars.put(&quot;queryDate&quot;, EncryptUtil.encryptAES(queryDate, key));
vars.put(&quot;bodyMsg&quot;, &quot;{\&quot;cipher\&quot;: \&quot;&quot; + bodyEncryptMsg + &quot;\&quot;}&quot;);

